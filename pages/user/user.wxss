.topView {
  width: 750rpx;
  height: 380rpx;
  background-image: url("data:image/png;base64,");
  background-size: 100% 100%;
  display: flex;
  justify-content: center
}

.topView .userInfo {
  width: 750rpx;
  height: 300rpx;
  display: flex;
  flex-direction: column;
  align-items: center
}

.topView .userInfo .userInfo-portrait {
  width: 150rpx;
  height: 150rpx;
  background-image: url("data:image/png;base64,");
  background-size: 100% 100%;
  position: relative
}

.topView .userInfo .userInfo-portrait image {
  width: 100rpx;
  height: 100rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  border-radius: 100px;
  -webkit-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
  z-index: 100
}

.topView .userInfo .userInfo-detail {
  font: 700 32rpx/60rpx Arial, Helvetica, sans-serif;
  color: #fff
}

.setting {
  width: 670rpx;
  -webkit-transform: translate(40rpx, -150rpx);
  transform: translate(40rpx, -150rpx);
  border-radius: 15rpx;
  z-index: 1024
}

.setting .setting-vip {
  width: 100%;
  height: 180rpx;
  background-image: url(data:image/png;base64,);
  background-size: 100% 100%;
  border-radius: 15rpx;
  position: relative;
  overflow: hidden;
  display: flex;
  flex-direction: row;
  align-items: center
}

.setting .setting-vip .setting-vip-absolute {
  position: absolute;
  top: 0;
  left: 15%;
  -webkit-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
  font-size: 150rpx;
  color: #242424;
  text-shadow: 2px 2px 2px #242424;
  font-weight: 900;
  -webkit-transform: rotate(-10deg);
  transform: rotate(-10deg);
  opacity: .1;
  z-index: 0
}

.setting .setting-vip .setting-vip-title {
  width: 50%;
  height: 60%;
  font-size: 45rpx;
  color: #ffa403;
  text-align: center;
  font-weight: 1000
}

.setting .setting-vip .setting-vip-sub {
  font-size: 30rpx;
  line-height: 30rpx;
  font-weight: 400;
  color: #fff
}

.setting .setting-vip .setting-vip-button {
  width: 50%;
  text-align: center;
  z-index: 1024
}

.setting .setting-vip .setting-vip-button .setting-vip-button-text {
  padding: 15rpx 35rpx;
  background: -webkit-linear-gradient(left, #fa0, #ff771d);
  border-radius: 100rpx 0 100rpx 100rpx;
  color: #fff;
  font-size: 30rpx
}

.list {
  width: 670rpx;
  margin-top: 30rpx
}

.list .list-item {
  width: 100%;
  height: 80rpx;
  padding: 0;
  border-bottom: .1px solid #f4f4f4;
  display: flex;
  flex-direction: row;
  align-items: center;
  position: relative;
  background-color: #fff;
}

.list .list-item .list-item-icon {
  margin-left: 20rpx;
  width: 50rpx;
  height: 50rpx
}

.list .list-item .list-item-title {
  width: 270rpx;
  font-size: 28rpx;
  padding-left: 20rpx;
  text-align: left;
}

.list .list-item .list-item-more {
  width: 250rpx;
  text-align: right;
  font-size: 28rpx;
  color: #555
}

.list .list-item .list-item-button {
  width: 100%;
  height: 100rpx;
  position: absolute;
  top: 0;
  left: 0;
  opacity: 0
}